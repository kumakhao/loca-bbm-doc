set terminal epslatex color size 6,5
set output "chapter3/picFrequency/frequencyPlot.tex"
reset
set boxwidth 0.9 relative
set style data histograms
set style histogram cluster
set style histogram clustered gap 3
set style fill solid 1.0 border lt-1
set yrange [0:0.5]
set key autotitle columnhead
plot 'chapter3/picFrequency/oval_data' using 2:xticlabels(1),\
'chapter3/picFrequency/serpentinen_data' using 2,\
'chapter3/picFrequency/clockwise_data' using 2,\
'chapter3/picFrequency/antiClockwise_data' using 2

