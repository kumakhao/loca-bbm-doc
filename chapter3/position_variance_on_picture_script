#set terminal pngcairo  transparent enhanced font "arial,10" fontscale 1.0 size 500, 350 
#set output 'contours.21.png'
#set key below
set terminal epslatex color size 6,5
set output "chapter3/position_variance_on_picture.tex"
set obj 10 circle fc rgb "red"
set obj 10 circle at graph 0.5,0.5   size graph .02  front
set xrange [-0.5:0.5]
set yrange [-0.5:0.5]
set size ratio 1.0
set zeroaxis
set border 0
set grid 
set xtics -0.5,0.1,0.5
set ytics -0.5,0.1,0.5
set tics scale 0.5
set xlabel "XAchse in Metern"
set ylabel "YAchse in Metern"
unset clabel
set view map
unset surface
set contour base
set dgrid3d 50,50 qnorm 4
splot "chapter3/position_variance_on_picture" with lines lc rgb "#000000" lw 3 t ""
